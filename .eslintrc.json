{
    "parserOptions": {
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "es6": true
    },
    "rules": {
        "one-var": [
            1,
            "never"
        ],
        "array-bracket-spacing": [
            1,
            "never"
        ],
        "block-scoped-var": [
            2
        ],
        "block-spacing": [
            1,
            "always"
        ],
        "consistent-return": [
            1
        ],
        "curly": [
            2
        ],
        "dot-location": [
            1,
            "property"
        ],
        "dot-notation": [
            1
        ],
        "guard-for-in": [
            2
        ],
        "init-declarations": [
            1,
            "always"
        ],
        "linebreak-style": [
            1,
            "windows"
        ],
        "newline-after-var": [
            1
        ],
        "no-alert": [
            1
        ],
        "no-catch-shadow": [
            1
        ],
        "no-debugger": [
            2
        ],
        "no-delete-var": [
            2
        ],
        "no-dupe-args": [
            2
        ],
        "no-dupe-keys": [
            2
        ],
        "no-duplicate-case": [
            2
        ],
        "no-empty": [
            1
        ],
        "no-eval": [
            1
        ],
        "no-extend-native": [
            2
        ],
        "no-extra-boolean-cast": [
            1
        ],
        "no-extra-parens": [
            1
        ],
        "no-extra-semi": [
            2
        ],
        "no-floating-decimal": [
            2
        ],
        "no-implicit-coercion": [
            2
        ],
        "no-implicit-globals": [
            1
        ],
        "no-implied-eval": [
            2
        ],
        "no-inner-declarations": [
            2
        ],
        "no-invalid-regexp": [
            2
        ],
        "no-invalid-this": [
            2
        ],
        "no-iterator": [
            2
        ],
        "no-labels": [
            2
        ],
        "no-lone-blocks": [
            2
        ],
        "no-lonely-if": [
            1
        ],
        "no-magic-numbers": [
            1
        ],
        "no-mixed-spaces-and-tabs": [
            1
        ],
        "no-multiple-empty-lines": [
            1,
            {
                "max": 1
            }
        ],
        "no-multi-spaces": [
            1
        ],
        "no-native-reassign": [
            2
        ],
        "no-new-func": [
            1
        ],
        "no-new-wrappers": [
            2
        ],
        "no-param-reassign": [
            1
        ],
        "no-proto": [
            2
        ],
        "no-redeclare": [
            2
        ],
        "no-return-assign": [
            2
        ],
        "no-self-assign": [
            2
        ],
        "no-self-compare": [
            2
        ],
        "no-shadow": [
            2
        ],
        "no-unexpected-multiline": [
            2
        ],
        "no-unreachable": [
            1
        ],
        "no-unused-expressions": [
            2
        ],
        "no-unused-vars": [
            1
        ],
        "no-useless-call": [
            1
        ],
        "no-useless-concat": [
            1
        ],
        "no-warning-comments": [
            1,
            {
                "terms": ["todo", "to do", "debugger", "debug"],
                "location": "anywhere"
            }
        ],
        "no-with": [
            2
        ],
        "one-var-declaration-per-line": [
            1
        ],
        "quotes": [
            1,
            "double"
        ],
        "radix": [
            1
        ],
        "semi": [
            2,
            "always"
        ],
        "use-isnan": [
            2
        ],
        "valid-jsdoc": [
            1
        ],
        "valid-typeof": [
            2
        ],
        "vars-on-top": [
            1
        ],
        "wrap-iife": [
            1
        ],
        "yoda": [
            1
        ]
    }
}
